{
  "name": "cypress-awesome",
  "version": "1.0.0",
  "description": "Projeto Cypress simples que acessa o Google",
  "scripts": {
    "test": "cypress run",
    "test:report": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=true,json=true",
    "generate-html": "marge cypress/reports/*.json -f report -o cypress/reports/html"
  },
  "devDependencies": {
    "cypress": "^14.5.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
